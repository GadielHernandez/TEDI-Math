        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Create the coins &amp; trinkets

// Get X &amp; Y corrdinates
x1 = x; y1 = y;
x2 = x + (sprite_get_width(sprite_area) * image_xscale);
y2 = y + (sprite_get_height(sprite_area) * image_yscale);

// Create the objects
if(created == false &amp;&amp; coins != 0 &amp;&amp; trinkets != 0){
    var i, o, xx, yy;
    // Create the coins
    for(i = 0; i &lt; coins; i++){
        var placement = false;
        while(placement == false){
            // Set a random position
            xx = irandom_range(x1, x2);
            yy = irandom_range(y1, y2);
            
            // Create a coin at that position
            o = instance_create(xx, yy, obj_money);
            
            // Check for collitions
            with(o){
                if(collision_circle(x, y, 55, obj_money, true, true)){
                    instance_destroy();
                }else{ // No collisions detected
                    //show_debug_message("NO COLLISION"); // (DEV)
                }
            }
            if(instance_exists(o)){
                placement = true;
            }
        }
        o.value = choose(1, 2, 5, 10);
        o.type = DRAGABLE_RETURN;
        o.xo = xx; o.yo = yy;
    }
    // Create the trinkets
    for(i = 0; i &lt; trinkets; i++){
        var placement = false;
        while(placement == false){
            // Set a random position
            xx = irandom_range(x1, x2);
            yy = irandom_range(y1, y2);
            
            // Create a coin at that position
            o = instance_create(xx, yy, obj_money);
            
            // Check for collitions
            with(o){
                if(collision_circle(x, y, 55, obj_money, true, true)){
                    instance_destroy();
                }else{ // No collisions detected
                    //show_debug_message("NO COLLISION"); // (DEV)
                }
            }
            if(instance_exists(o)){
                placement = true;
            }
        }
        o.value = TRINKET;
        o.type = DRAGABLE_RETURN;
        o.xo = xx; o.yo = yy;
    }
    
    // All coins &amp; trinkets are created
    created = true;
}
</string>
          </argument>
